# Prime Check Program
#
#   The Program takes a number and returns True 
#   if the number is a prime otherwise False.

# Number to check
let number = 11


let index = 1
let divisors = 0

while (index <= number) {
    if (number % index == 0) {
        let divisors = divisors + 1
    }
    let index = index + 1
}

# Result
divisors == 2