let number = 5879  
let is_prime = true

if number < 2 {
    is_prime = false
} else {
    let divisor = 2
    while divisor <= number / 2 {
        if number % divisor == 0 {
            is_prime = false
            divisor = number 
        } else {
            divisor = divisor + 1
        }
    }
}

(is_prime)
